<config>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <vubla_tweaks translate="title" module="vubla">
                                    <title>Tweaks</title>
                                    <sort_order>1</sort_order>
                                        <children>
                                            <searchautocomplete translate="title" module="searchautocomplete">
                                                <title><![CDATA[Search Autocomplete]]></title>
                                            </searchautocomplete>
                                        </children>
                                    </vubla_tweaks>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>